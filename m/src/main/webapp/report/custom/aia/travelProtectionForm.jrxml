	<queryString>
		<![CDATA[select po.ref_no
, po.purchase_date
, users.name as user_name
, users.surname as user_surname
, por1.initial_text
, por1.name
, por1.surname
, por1.age
, por1.home_phone
, por1.office_phone
, por1.mobile_phone
, por1.current_address_line1
, subd1.name as subdistrict_name
, dist1.name as district_name
, prov1.name as province_name
, subd1.postal_code
, por1.idno
, por1.idcard_issue_district
, por1.idcard_issue_province
, por1.dob
, por1.height
, por1.weight
, por1.nationality
, por1.occupation
, por1.position
, por1.jobdescription
, por1.income
, por1.fx1
, por1.fx2
, por1.fx11
, por1.fx12
, por1.fx13
, por1.fx15
, por1.fx19
, por1.fx16
, por1.fx20
, por1.fx17
, por1.fx21
, DATEADD(day,1,po.purchase_date) as date1
, DATEADD(year,1,DATEADD(day,1,po.purchase_date)) as year1
, p.name as product_name
, pp.code as product_code
, po.net_premium
, po.card_type
, po.card_issuer
, po.card_no
, po.card_expiry_month
, po.card_expiry_year
, po.card_holder_name
, pp.code as product_plan_code
, pp.no as product_plan_no
, pp.payment_mode as product_plan_payment_mode
from purchase_order as po
inner join users on users.id = po.create_by_id
inner join purchase_order_detail as pod on po.id = pod.purchase_order_id
inner join purchase_order_register as por1 on pod.id = por1.purchase_order_detail_id and por1.no = 1
left join subdistrict as subd1 on subd1.id = por1.current_subdistrict_id
inner join district as dist1 on dist1.id = subd1.district_id
inner join province as prov1 on prov1.id = dist1.province_id
inner join product as p on p.id = pod.product_id
inner join product_plan as pp on pp.id = pod.product_plan_id
where po.sale_result = 'Y'
and po.approval_status = 'approved'
and po.payment_status = 'approved'
and po.qc_status = 'approved'
and po.sale_date between $P{fromDate} and $P{toDate}]]>
